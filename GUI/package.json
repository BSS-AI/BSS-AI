{
  "name": "bss-ai-gui",
  "version": "0.0.1",
  "description": "BSS AI Macro GUI",
  "main": "main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "start": "electron .",
    "dist": "electron-builder",
    "setup": "npm install && node setup/setup.js && npm run build && npm start",
    "quick": "npm run build && npm start"
  },
  "keywords": [],
  "author": "BSS AI",
  "type": "commonjs",
  "license": "ISC",
  "devDependencies": {
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "electron": "^37.2.4",
    "electron-builder": "^26.0.12",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "vite": "^7.1.2",
    "vite-plugin-javascript-obfuscator": "^3.1.0"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "bytenode": "^1.5.7",
    "crypto": "^1.0.1",
    "electron-store": "^8.2.0",
    "ini": "^4.1.2",
    "node-fetch": "^2.7.0",
    "os": "^0.1.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "build": {
    "asar": true,
    "asarUnpack": [],
    "appId": "com.bssai.gui",
    "productName": "BSS AI GUI",
    "files": [
      "main.js",
      "index.js",
      "compiled/**/*",
      "preload.js",
      "public/**/*",
      "assets/**/*",
      "start.bat",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "portable",
      "icon": "assets/bssAiLogo.ico"
    }
  }
}
